FUNCTION mod :
PARAM x
PARAM n
t2 := x
t6 := x
t7 := n
t4 := t6 / t7
t5 := n
t3 := t4 * t5
t1 := t2 - t3
RETURN t1
FUNCTION DigitSum :
PARAM y
t8 := y
t9 := #0
IF t8 == t9 GOTO L1
GOTO L2
LABEL L1 :
t10 := #0
RETURN t10
LABEL L2 :
t14 := y
t15 := #10
ARG t15
ARG t14
t12 := CALL mod
t17 := y
t18 := #10
t16 := t17 / t18
ARG t16
t13 := CALL DigitSum
t11 := t12 + t13
RETURN t11
FUNCTION main :
READ t20
num := t20
t21 := num
t22 := #0
IF t21 < t22 GOTO L3
GOTO L4
LABEL L3 :
t25 := #1
t24 := #0 - t25
WRITE t24
GOTO L5
LABEL L4 :
t28 := num
ARG t28
t27 := CALL DigitSum
WRITE t27
LABEL L5 :
t29 := #0
RETURN t29
